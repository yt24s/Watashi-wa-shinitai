-- Scripts:
 
local function HNULUA_fake_script() -- TextLabel.LocalScript 
    local script = Instance.new('LocalScript', TextLabel)
 
    Stopwatch = 0
    while true do
        Stopwatch = Stopwatch+1
        wait(1)
        script.Parent.Text = (Stopwatch)
    end
end
coroutine.wrap(HNULUA_fake_script)()
end
